import java.util.*;
class Main
{
    public static void main(String[] args)
    {
        int n,fact=1;
        Scanner obj=new Scanner(System.in);
        System.out.println("Enter the numbers :");
        n=obj.nextInt();
        for(int i=1; i<=n; i++)
        {
            fact=fact*i;
        }
            System.out.println("fact"+fact);
    }
}
