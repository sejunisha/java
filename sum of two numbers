import java.util.*;
class Main 
{
    public static void main(String[] args) 
    {
        Scanner obj=new Scanner(System.in);
        System.out.println("Enter the first numbers :");
        int a=obj.nextInt();
        System.out.println("Enter the second numbers :");
        int b=obj.nextInt();
        int sum=a+b;
        System.out.println("sum :"+sum);
    }
}
