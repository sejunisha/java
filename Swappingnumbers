class swappingnumbers
{
    public static void main(String[] args)
    {
        int x=10;
        int y=90;
        
        System.out.println("Before swapping :" +x);
        System.out.println("Before swapping :" +y);
        
        x=x+y;
        y=x-y;
        x=x-y;
        
        System.out.println("After swapping :" +x);
        System.out.println("After swapping :" +y);
    }
}
